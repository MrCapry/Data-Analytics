    "# Number to binary\n",
    "\n",
    "def binary(decimal):\n",
    "    binary_str = ''\n",
    "    while decimal>0:\n",
    "        reminder = decimal % 2\n",
    "        binary_str = str(reminder) + binary_str\n",
    "        decimal //=2\n",
    "    return binary_str if binary_str else '0'\n",
    "\n",
    "testing_t = [1,5,10,8,16,11]\n",
