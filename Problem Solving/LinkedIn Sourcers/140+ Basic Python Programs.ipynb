    "# Program 106\n",
    "# Remove all occurance and add to end\n",
    "\n",
    "def move_to_end(lst,element):\n",
    "    cnting = lst.count(element)\n",
    "    ''' ცუდი პრაქტიკა არის\n",
    "    for elem in lst:\n",
    "        if elem==element:\n",
    "            lst.remove(elem)\n",
    "    lst.extend([element]*cnting)\n",
    "    return lst\n",
    "    '''\n",
    "    lst = [x for x in lst if x!=element]\n",
    "    lst.extend([element]*cnting)\n",
    "    return lst\n",
    "    \n",
    "testing = [9,2,9,9,3,3,9]\n",
    "move_to_end(testing,9)"
